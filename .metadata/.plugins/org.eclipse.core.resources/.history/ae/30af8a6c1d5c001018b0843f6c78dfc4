package ch.hsr.waktu.domain;

import javax.persistence.GeneratedValue;


public class User {
	@id
	@GeneratedValue
	private int id;
	
	private String name;

	public User(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public String toString() {
		return name;
	}

}
